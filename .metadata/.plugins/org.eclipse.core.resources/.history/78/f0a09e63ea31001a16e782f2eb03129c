	<script type="text/javascript">
	function printText(){
		input = amnt.test.value; //getter
		//alert(myval); 테스트 확인 후 주석처리
		result = (input*12900*0.9);
		document.getElementById("result").innerHTML = result;
	}
	</script>
</head>
<body>
	<form name="amnt">
			<label>수량: </label> 
			<input type="text" name="test" onkeyup="printText()">
