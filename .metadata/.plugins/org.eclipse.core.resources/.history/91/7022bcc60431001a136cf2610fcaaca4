<title>Insert title here</title>
</head>
<body>
	<script type="text/javascript">
		jumsu = 90;
		alert(jumsu/10);
		if(jumsu>100|jumsu<0){
			document.write("잘못입력하셨습니다.")
		}else{
	
			switch(jumsu)
			case 90:
				document.write("수");
			case 80:
				document.write("우");
			case 70:
				document.write("미");
			case 60:
				document.write("양");
			default
				document.write("가");
		}
			

	</script>
	
</body>
</html>